
import java.io.Serializable;


public class Message implements Serializable{
    
    private static final long serialVersionUID = 1L;
    public String type, expediteur, contenu, destinataire;
    
    public Message(String type, String expediteur, String contenu, String destinataire){
    	
        this.type = type; this.expediteur = expediteur; this.contenu = contenu; this.destinataire = destinataire;
    }
    
    @Override
    public String toString(){
        return "{type='" + type + "', expediteur='" + expediteur+"', contenu='" + contenu + "', destinataire='" + destinataire + "'}";
    }
}

